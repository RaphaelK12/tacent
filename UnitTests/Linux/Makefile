.PHONY: clean All

All:
	@echo "----------Building project:[ Image - Debug ]----------"
	@cd "/mnt/c/GitHub/tacent/Modules/Image/Linux" && "$(MAKE)" -f  "Image.mk"
clean:
	@echo "----------Cleaning project:[ Image - Debug ]----------"
	@cd "/mnt/c/GitHub/tacent/Modules/Image/Linux" && "$(MAKE)" -f  "Image.mk" clean
